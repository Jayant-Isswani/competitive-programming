#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <queue>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    queue<int> qu;
    int q;
    cin>>q;
    int i;
    for(i=0;i<q;i++)
        {
        int type;
        cin>>type;
        if(type==1)
            {
            int element;
            cin>>element;
            qu.push(element);
        }
        if(type==2)
            qu.pop();
        if(type==3)
            cout<<qu.front()<<endl;
    }
    
    return 0;
}

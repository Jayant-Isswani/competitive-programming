#include <iostream>
using namespace std;
 
int main() {
    int t;
    cin>>t;
    while(t--)
    {
    	int n,c;
    	cin>>n>>c;
    	int sum=0;
    	int element;
    	for(int i=0;i<n;i++)
    	{
    	    cin>>element;
    	    sum+=element;
    	}
    	if(sum<=c)
    	{
    	    cout<<"Yes"<<endl;
    	}
    	else
    	{
    	    cout<<"No"<<endl;
    	}
    }
    return 0;
} 

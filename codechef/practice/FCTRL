#include <iostream>
#include <cstdio>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n,count=0;
	    scanf("%d",&n);
	    int k=5;
	    while((int)n/k)
	    {
	       count+=n/k;
	       k=k*5;
	    }
	    printf("%d\n",count);
	}
	return 0;
}
